/dts-v1/;

#include "stm32mp135f-dk.dts"

/ {
	model = "STMicroelectronics STM32MP135F-DK-OSTL Discovery Board";
	compatible = "st,stm32mp135f-dk-ostl", "st,stm32mp135f-dk", "st,stm32mp135";
};

&rcc {
